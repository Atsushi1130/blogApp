<div class="contact_form">
	<p class="contact_title">お問い合わせ</p>
	<%= form_with model: @inquery, url: "/contact" do |f| %>
		<p>名前</p>
		<p class="name"><%= f.text_field :name %></p>
		<p>メールアドレス</p>
		<p class="email"><%= f.text_field :email %></p>
		<p>お問い合わせ内容</p>
		<p class="message"><%= f.text_area :message %></p>
		<p class="submit"><%= f.submit "送信" %></p>
	<% end %>
</div>
